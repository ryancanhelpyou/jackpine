{# desktop #}
<nav id="nav-main" class="nav-main" role="navigation">
    <div class="flex items-end justify-between p-6 w-11/12 mx-auto">
        <div class="block w-1/5">
            <a href="{{ site.url|e('esc_url') }}" title="Home" rel="home">
            <picture>
                <source srcset="{{ custom_logo_url|towebp }}" type="image/webp">
                <source srcset="{{ custom_logo_url|tojpg }}" type="image/jpeg">
                <img alt="logo" class="w-full transition duration-500 ease-in-out transform hover:scale-105" src="{{ custom_logo_url|resize(400, 300) }}" srcset="{{ custom_logo_url|resize(200, 150)|retina(1) }} 1x,
                    {{ custom_logo_url|resize(800, 600)|retina(2) }}  2x,
                    {{ custom_logo_url|resize(1600, 1200)|retina(3) }}  3x,
                    {{ custom_logo_url|resize(2400, 2400)|retina(4) }}  4x">
            </picture>
            </a>
        </div>
        <div class="bg-gray-300 text-gray-800 font-light text-xl flex flex-row">
            {# <a href="" class="mr-5 hover:text-orange">about</a>
            <a href="" class="mr-5 hover:text-orange">covid-19</a>
            <a href="" class="mr-5 hover:text-orange">services</a>
            <a href="" class="mr-5 hover:text-orange">products</a>
            <a href="" class="mr-5 hover:text-orange">for patients</a>
            <a href="" class=" hover:text-orange">contact</a> #}

                {% if items|default(top_menu.items) %}
                    {% for item in items|default(top_menu.items) %}
                        {% if item.children %}
                            <div x-data="{ hover: false }" class="relative">
                                <a id="menu-item-{{ item.id }}" href="{{ item.link }}" class="ml-6 hover:text-orange flex items-center" @mouseenter="{hover=true}" @mouseleave="{hover=false}">{{ item.name }}</a>
                                <ul id="menu-item-list-{{ item.id }}" class="absolute ml-4 p-2 bg-white border-gray-400 border-.5 flex flex-col whitespace-no-wrap" x-show="hover" @mouseenter="{hover=true}" @mouseleave="{hover=false}">
                                    {% for child in item.children %}
                                        <li id="menu-item-child-{{ child.id }}" class="p-2">
                                            <a href="{{ child.link }}" class="hover:text-orange">{{ child.title }}</a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </div>
                        {%  else %}
                            <a id="menu-item-{{ item.id }}" href="{{ item.link }}" class="ml-6 hover:text-orange">{{ item.name }}</a>
                        {% endif %}
                    {% endfor %}
                {% endif %}
        </div>
        <div class="bg-gray-300 flex items-center justify-end">
            <a href=""><svg xmlns="http://www.w3.org/2000/svg" class="w-8 text-gray-400 hover:text-orange mr-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z" />
            </svg></a>
            <button class="bg-transparent hover:bg-orange text-orange hover:text-gray-100 py-2 px-4 border border-orange hover:border-transparent rounded">schedule appointment</button
        </div>
    </div>
